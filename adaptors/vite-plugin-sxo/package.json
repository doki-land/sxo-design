{
    "name": "vite-plugin-sxo",
    "version": "0.0.0",
    "description": "Vite plugin for SXO Design System with auto-import and HMR support.",
    "keywords": [
        "sxo",
        "design-system",
        "ui",
        "components"
    ],
    "license": "MIT",
    "author": "sxo team",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        }
    },
    "publishConfig": {
        "access": "public",
        "main": "./dist/index.cjs",
        "module": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "require": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch",
        "test": "vitest run"
    },
    "dependencies": {
        "@sxo/design": "workspace:*",
        "@sxo/engine": "workspace:*",
        "vite": "^6.2.1"
    },
    "peerDependencies": {
        "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
    },
    "packageManager": "pnpm@9.15.2",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/doki-land/sxo-engine.git",
        "directory": "adaptors/vite-plugin-sxo"
    },
    "bugs": {
        "url": "https://github.com/doki-land/sxo-engine/issues"
    },
    "homepage": "https://github.com/doki-land/sxo-engine/tree/main/adaptors/vite-plugin-sxo#readme",
    "devDependencies": {
        "typescript": "^5.7.3",
        "vite": "^6.2.1",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.0.2"
    }
}
